page {
  background-color: rgba(245, 248, 250, 1);
  position: relative;
  margin-bottom: 48px;
  padding-bottom: env(safe-area-inset-bottom);
}

.dialog {
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
  z-index: 5;
  background-color: rgba(0, 0, 0, 0.4);
  display: flex;
  flex-flow: column wrap;
  align-items: center;
  justify-content: center;
}

.dialog .dialogImgBox {
  width: 85vw;
  height: 121.428vw;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  overflow: hidden;
}

.dialogImgBox image {
  width: 100%;
  height: 100%;
}

.dialog .dialogClose {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.dialog .icon-guanbi1 {
  font-size: 60rpx;
  color: #f5f5f5;
  z-index: 5;
}

button {
  border-radius: 0;
  font-size: 30rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  margin: 0;
  background-color: transparent;
}

button::after {
  border: none;
}

.btnhover {
  background-color: transparent;
}

.contact {
  position: fixed;
  bottom: 100rpx;
  right: 20rpx;
  width: 120rpx;
  height: 120rpx;
}

.contact image {
  width: inherit;
  height: inherit;
  display: block;
}

.container-content {
  width: 100vw;
  min-height: 100vh;
  position: relative;
}

