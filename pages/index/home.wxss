.bannerSwiper {
  width: 100vw;
  height: 460rpx;
}

.bannerSwiper image {
  width: 100vw;
  height: 460rpx;
}

.still-searchBox{
	width: 100%;
	background-color: white;
	box-sizing: border-box;
	padding: 14rpx 24rpx;
}

.still-searchBox .still-search{
	width: 100%;
	height: 60rpx;
	border-radius: 30rpx;
	display: inline-flex;
	flex-flow: row nowrap;
	align-items: center;
	justify-content: center;
	background-color: rgba(246, 246, 246, 1);
	color: rgb(182, 182, 182);
	font-size: 28rpx;
}

.still-search .iconfont{
	color: rgba(112, 112, 112, 1);
	font-size: 40rpx;
	line-height: 40rpx;
	margin-right: 15rpx;
}

.tabBar-top{
	display: flex;
	width: 100%;
	height: 70rpx;
	box-sizing: border-box;
	flex-flow: row nowrap;
	align-items: center;
	justify-content: center;
	background-color: rgb(250, 250, 250);
	color: rgba(112, 112, 112, 1);
	font-weight: 200; 
	box-shadow: 0 3rpx 10rpx rgba(122, 122, 122, 0.1);
}

.tabBar-top .tabBarItem{
	height: 100%;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	padding: 0 15rpx;
	position: relative;
	font-size: 28rpx;
}

.active{
	font-weight: 600;
	color: rgb(48, 47, 47);
}

.community {
  width: 100vw;
  box-sizing: border-box;
  padding-bottom: 40rpx;
}
