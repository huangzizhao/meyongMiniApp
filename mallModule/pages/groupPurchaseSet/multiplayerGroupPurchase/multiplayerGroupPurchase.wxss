/* pages/beforeDoubleGroupPurchase/beforeDoubleGroupPurchase.wxss */

.app {
  box-sizing: border-box;
  background-color: rgba(246, 246, 246, 1);
}

.purchaseTab {
  background-color: #ff3f61;
  background-image: linear-gradient(to bottom, rgb(255, 63, 97), rgb(222, 34, 29));
  width: 100vw;
  box-sizing: border-box;
  padding: 30rpx 35rpx;
}

.purchaseTab .tabNav {
  background-color: #fff;
  border-radius: 10rpx;
  padding: 0 20rpx;
  box-shadow: 0px 2px 9px rgba(0, 0, 0, 0.2);
  position: relative;
  overflow: hidden;
  z-index: 3;
}

.tabNav .stamp {
  position: absolute;
  right: -25rpx;
  top: 60rpx;
  width: 190rpx;
  height: 190rpx;
  box-sizing: border-box;
  transform: rotate(-35deg);
  z-index: 2;
}

.stampNav {
  position: relative;
  width: 190rpx;
  height: 190rpx;
  border: 5rpx solid rgb(228, 57, 57);
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: bold;
  color: rgb(228, 57, 57);
  border-radius: 50%;
}

.stampNav::before {
  content: '';
  position: absolute;
  width: 165rpx;
  height: 165rpx;
  border-radius: 50%;
  border: 2rpx solid rgb(228, 57, 57);
}

button {
  border-radius: 0;
  font-size: 30rpx;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  margin: 0;
  background-color: transparent;
}

button::after {
  border: none;
}

.purchaseActivity {
  height: 280rpx;
  box-sizing: border-box;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  border-bottom: 1rpx solid rgb(219, 219, 219);
}

.activityCover {
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
  border-radius: 8rpx;
  border: 0.5rpx solid rgba(0, 0, 0, 0.1);
}

.activityCover image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.activityInfo {
  height: 200rpx;
  flex: 1;
  display: flex;
  flex-flow: row wrap;
  align-content: space-around;
  font-size: 28rpx;
  margin-left: 20rpx;
}

.infoTop {
  display: inline-flex;
  flex-flow: row nowrap;
  align-items: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.infoTop .tag {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  color: #ff3f61;
  font-size: 17rpx;
  border-radius: 3rpx;
  padding: 0 8rpx;
  border: 0.5px solid #ff3f61;
  box-sizing: border-box;
  margin-right: 5rpx;
}

.infoMid {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.infoBottom {
  width: 100%;
  display: flex;
  justify-content: space-between;
  color: rgba(240, 37, 15, 1);
  font-size: 20rpx;
}

.infoBottom .priceNum {
  font-size: 40rpx;
}

.purchaseState {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  align-content: space-between;
  background-color: #fff;
}

.purchaseState .stateTitle {
  font-size: 35rpx;
  font-weight: bold;
}

.purchaseState .imgList {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 84rpx;
  margin: 30rpx 0 20rpx 0;
}

.imgList .imgItem {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  box-sizing: border-box;
  border: 0.5px dashed rgb(216, 216, 216);
  margin: 0 6rpx;
  background-color: rgba(248, 248, 248, 1);
  overflow: hidden;
}

.imgItem image {
  width: 100%;
  height: 100%;
}

.btnList {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  background-color: #fff;
}

.btnList button {
  margin: 15rpx 10rpx 0 10rpx;
  flex: 1;
  height: 70rpx;
  border-radius: 35rpx;
  font-size: 30rpx;
  color: #fff;
  background-image: linear-gradient(to right, rgb(254, 78, 23), rgb(241, 0, 0));
  box-shadow: 0px 2px 9px rgba(241, 0, 0, 0.2);
}

.project {
  width: 100vw;
}

.moreProjectTitle {
  padding: 25rpx 20rpx 20rpx;
  font-size: 30rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.otherProjectList {
  width: 100vw;
  box-sizing: border-box;
  padding: 0 10rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
  margin-bottom: 50rpx;
}

.noOtherProjectList {
  width: 100vw;
  box-sizing: border-box;
  padding: 0 10rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  margin-bottom: 50rpx;
}

.noOtherProjectList .nothing {
  width: 250rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  color: rgb(182, 182, 182);
  font-size: 30rpx;
}

.nothing .iconfont {
  font-size: 80rpx;
}

.otherProjectList .projectItem {
  background-color: #fff;
  width: calc((100vw - 60rpx) / 2);
  margin: 0 10rpx 15rpx;
  border-radius: 15rpx;
  overflow: hidden;
}

.projectItem image {
  width: 100%;
  height: 300rpx;
  display: block;
}

.itemInfo {
  box-sizing: border-box;
  padding: 10rpx;
}

.itemInfo .projectItemTitle {
  font-size: 25rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-weight: bold;
}

.itemInfo .projectItemPrice {
  color: #ff3f61;
  font-size: 25rpx;
  margin: 10rpx 0;
  font-weight: bold;
}

.itemInfo .openGroup {
  height: 50rpx;
  width: 100%;
  font-size: 25rpx;
  border-radius: 25rpx;
  background-image: linear-gradient(to right, rgb(254, 164, 26), rgb(254, 128, 11));
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.purchaseProcess {
  background-color: #fff;
  box-sizing: border-box;
  padding: 25rpx 35rpx;
}

.purchaseProcessTitle {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  font-size: 28rpx;
  margin-bottom: 20rpx;
}

.description {
  margin-top: 20rpx;
  width: 100%;
  display: flex;
  justify-content: flex-start;
  font-size: 22rpx;
  color: rgb(189, 185, 185);
}

.purchaseProcess .iconList {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
}

.iconList .iconfont {
  color: rgb(199, 196, 196);
  font-size: 50rpx;
  font-weight: 200;
}

.iconList .line {
  height: 1px;
  width: 50rpx;
  background-color: rgb(199, 196, 196);
  margin: 0 37rpx;
}

.purchaseProcess .processText {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.processText text {
  font-size: 20rpx;
  color: rgb(100, 99, 99);
  margin: 0 25rpx;
}
