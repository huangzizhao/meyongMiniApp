/* pages/coupon/coupon.wxss */
.app{
	box-sizing: border-box;
	background-color: #fff;
	width: 100vw;
	min-height: 100vh;
	padding:10rpx 25rpx 0rpx 25rpx;
}
.cp-carton {
  box-sizing: border-box;
  width: 100%;
  height: 250rpx;
  border-radius: 10rpx;
  margin-bottom: 21rpx;
}
.cp-carton:last-child{
margin: 0rpx;
}

.cp-carton image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

